<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Pruebita Go</title>
    <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/assets/fonts/fonts.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/icons/icomoon/icomoon.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/core.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap-extended.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/plugins.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/css/color-system.css")" rel="stylesheet" type="text/css" />
</head>
<body style="height:100%; background:url('@Url.Content("~/Content/assets/images/assets/login_bg.jpg")') no-repeat 0 0; background-size:cover;">
    <div class="login-container material">

        <!-- Page content -->
        <div class="page-content">
            @RenderBody()
            <!-- Footer -->
            <div class="footer text-size-mini">
                &copy; 2016 Pruebitas Go &nbsp;&nbsp;Version - 1.0.0
            </div>
            <!-- /footer -->

        </div>
        <!-- /page content -->

    </div>

        <script src="@Url.Content("~/Content/js/jquery.js")" type="text/javascript"></script>

        <script src="@Url.Content("~/Content/js/bootstrap.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/js/forms/uniform.min.js")" type="text/javascript"></script>
        <script>
            $(function () {
                $(".styled, .multiselect-container input").uniform({
                    radioClass: 'choice'
                });
                $('input,textarea').focus(function () {
                    $(this).data('placeholder', $(this).attr('placeholder'))
                           .attr('placeholder', '');
                }).blur(function () {
                    $(this).attr('placeholder', $(this).data('placeholder'));
                });
                $("input").attr("autocomplete", "off");
            });
        </script>

        @RenderSection("scripts", required: false)
    </body>
</html>



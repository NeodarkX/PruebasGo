@using PRUEBITASGO.Security


<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Pruebita Go</title>
    <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/core.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/layout.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap-extended.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/components.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/plugins.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/loaders.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/color-system.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/fancybox/jquery.fancybox.css")" rel="stylesheet" type="text/css" />
</head>
<body class="material-menu" id="top">
    <header class="main-nav clearfix">
        <div class="top-search-bar">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6 col-md-offset-3">
                        <div class="search-input-addon">
                            <span class="addon-icon"><i class="icon-search4"></i></span>
                            <input type="text" class="form-control top-search-input" placeholder="Search">
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="navbar-left pull-left">
            <div class="clearfix">
                <ul class="left-branding pull-left">
                    <li><span class="left-toggle-switch visible-handheld" style="touch-action: pan-y; -webkit-user-select: none; -webkit-user-drag: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0);"><i class="icon-menu7"></i></span></li>
                    <li>
                        <a href=""><div class="logo"></div></a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="navbar-right pull-right">
            <div class="clearfix">
                <ul class="pull-right top-right-icons">
                    <li class="dropdown user-dropdown">
                        <a href="" class="btn-user dropdown-toggle hidden-xs" data-toggle="dropdown"><img class="img-circle user" alt=""></a>
                        <a href="" class="dropdown-toggle visible-xs" data-toggle="dropdown"><i class="icon-more"></i></a>
                        <div class="dropdown-menu">
                            <h5 class="text-center"><b>Hola! @SessionPersister.Username</b></h5>
                            <div class="text-center"><a href="@Url.Action("Logout","Account")" class="btn btn-sm btn-info"><i class="icon-exit3 i-16 position-left"></i> Logout</a></div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <aside class="sidebar" style="height: 652px;">
        <div class="slimScrollDiv" style="position: relative; overflow: hidden; width: 250px; height: 652px;">
            <div class="left-aside-container" style="overflow: hidden; width: 250px; height: 652px;">
                <div class="user-profile-container">
                    <div class="user-profile clearfix">
                        <div class="admin-user-thumb">
                            <img alt="admin" class="img-circle">
                        </div>
                        <div class="admin-user-info">
                            <ul class="user-info">
                                <li><a href="" class="text-semibold text-size-large">@SessionPersister.Username</a></li>
                                <li><a href=""><small></small></a></li>
                            </ul>
                            <div class="logout-icon"><a href="login_simple.html"><i class="icon-exit2"></i></a></div>
                        </div>

                    </div>
                </div>
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active" id="tab-menu"><a href="menu" aria-controls="menu" role="tab" data-toggle="tab"><i class="icon-home2"></i></a></li>
                 </ul>

                <div class="tab-content">


                    <div role="tabpanel" class="tab-pane active fadeIn" id="menu">
                        <ul class="sidebar-accordion">

                            <li class="list-title">Principal</li>
                            <li>
                                <a href="" class="acc-parent active"><i class="icon-portfolio"></i><span class="list-label"> Productos</span><span class="acc-icon"></span></a>
                                <ul style="display: none;">
                                    <li><a href="@Url.Action("Index","Products")">Listar Producto</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="" class="acc-parent "><i class="icon-stack4"></i><span class="list-label"> Categorias</span><span class="acc-icon"></span></a>
                                <ul style="display: none;">
                                    <li><a href="@Url.Action("Index","Categories")">Listar Categorias</a></li>
                                </ul>
                            </li>                           

                        </ul>
                    </div>


                </div>
            </div><div class="slimScrollBar" style="background: rgb(144, 164, 174); width: 7px; position: absolute; top: 0px; opacity: 0.7; display: none; border-radius: 3px; z-index: 99; right: 0px; height: 155.147px;"></div><div class="slimScrollRail" style="width: 7px; height: 100%; position: absolute; top: 0px; border-radius: 0px; background: rgba(255, 255, 255, 0.0980392); opacity: 0.5; z-index: 90; right: 0px; display: none;"></div>
        </div>
    </aside>
    <section class="main-container">
        @RenderBody()
        <!--Page Header-->
        <!--<div class="header">
            <div class="header-content">
                <div class="page-title">
                    <i class="icon-file-empty position-left"></i> Empty Page <small>(Starter page)</small>
                </div>
                <ul class="breadcrumb">
                    <li><a href="index-2.html">Home</a></li>
                    <li class="active">Empty Page</li>
                </ul>
            </div>
        </div>

        <div class="container-fluid page-content">
           
        </div>-->

        <!--Footer -->
        <footer class="footer-container">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <div class="footer-left">
                            <span>
                                © 2016 Pruebitas Go&nbsp;&nbsp;&nbsp;•&nbsp;&nbsp;&nbsp; Version - 1.0.0
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!--/Footer-->

    </section>




    <script src="@Url.Content("~/Content/js/jquery.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Content/js/bootstrap.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Content/js/jquery.ui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/nav.accordion.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/hammerjs.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.hammer.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/scrollup.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.slimscroll.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/smart-resize.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/blockui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/wow.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/fancybox.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/venobox.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/forms/uniform.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/forms/switchery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/forms/select2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/core.js")" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $("input").attr("autocomplete", "off");
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>

